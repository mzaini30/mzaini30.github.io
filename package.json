{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@roxi/routify": "^2.7.3",
    "btoa": "^1.2.1",
    "btoa-pro": "github:mzaini30/btoa-pro",
    "embed-youtube": "github:mzaini30/embed-youtube",
    "form-data": "^3.0.0",
    "highlight.js": "^10.4.1",
    "marked": "^4.0.10",
    "node-fetch": "^2.6.1",
    "nollup": "^0.13.13",
    "rollup": "^2.3.4",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "vconsole": "^3.3.4"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "scripts": {
    "build": "routify -b --dynamic-imports && rollup -c",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "rollup -c -w --environment LIVERELOAD",
    "dev:nollup": "nollup -c --hot --history-api-fallback --port 5000 --content-base ./public",
    "dev": "routify -c dev:nollup",
    "start": "sirv public --single"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  }
}
